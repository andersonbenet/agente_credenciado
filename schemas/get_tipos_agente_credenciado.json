{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "array",
  "title": "The Root Schema",
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "required": [
      "id",
      "descricao"
    ],
    "properties": {
      "id": {
        "$id": "#/items/properties/id",
        "type": "integer",
        "title": "The Id Schema",
        "default": 0,
        "examples": [
          1
        ]
      },
      "descricao": {
        "$id": "#/items/properties/descricao",
        "type": "string",
        "title": "The Descricao Schema",
        "default": "",
        "examples": [
          "VEICULOS"
        ],
        "pattern": "^(.*)$"
      }
    }
  }
}